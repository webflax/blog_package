{
    "name": "webflaxco/blog",
    "description": "a Complate Blog Package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
